{
    "name": "vits/laravel-svelme",
    "description": "Helper library for building InertiaJS applications",
    "version": "0.4.1",
    "license": "MIT",
    "authors": [
        {
            "name": "Vitauts Stoƒçka",
            "email": "vs@vits.lv"
        }
    ],
    "type": "library",
    "keywords": [
        "laravel",
        "inertiajs"
    ],
    "autoload": {
        "psr-4": {
            "Vits\\Svelme\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vits\\Svelme\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "~8.2",
        "illuminate/database": "^10.0|^11.0",
        "inertiajs/inertia-laravel": "^1.3|^2.0"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^9.2",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-watch": "^2.1",
        "spatie/laravel-data": "^4.7"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vits\\Svelme\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    }
}